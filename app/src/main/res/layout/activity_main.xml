<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <Button
        android:id="@+id/button1"
        android:layout_width="171dp"
        android:layout_height="99dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="EN/中文"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/button2"
        android:layout_width="171dp"
        android:layout_height="99dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="维护模式"
        android:textSize="30dp"
        app:layout_constraintStart_toEndOf="@+id/button1"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/button3"
        android:layout_width="171dp"
        android:layout_height="99dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="故障反馈"
        android:textSize="30dp"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/button4"
        android:layout_width="171dp"
        android:layout_height="99dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:text="语音提示"
        android:textSize="30dp"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="171dp"
        android:layout_height="99dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="GO运行"
        android:textSize="30dp"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toTopOf="parent" />
            <ImageButton
                android:id="@+id/arc1"
                android:layout_width="323dp"
                android:layout_height="323dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="152dp"
                android:background="@drawable/ke"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

            <ImageButton
                android:id="@+id/arc2"
                android:layout_width="323dp"
                android:layout_height="323dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="152dp"
                android:background="@drawable/qita"
                app:layout_constraintStart_toEndOf="@+id/arc1"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/arc3"
        android:layout_width="323dp"
        android:layout_height="323dp"
        android:layout_marginTop="152dp"
        android:background="@drawable/chuyu"
        app:layout_constraintStart_toEndOf="@+id/arc2"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
                android:id="@+id/arc4"
                android:layout_width="323dp"
                android:layout_height="323dp"
                android:layout_marginStart="0dp"
                android:layout_marginTop="152dp"
                android:background="@drawable/youhai"
                app:layout_constraintStart_toEndOf="@+id/arc3"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/kehuishou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginTop="8dp"
        android:text="可回收垃圾"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arc1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="736dp"
        android:layout_marginTop="8dp"
        android:text="厨余垃圾"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arc3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1068dp"
        android:layout_marginTop="8dp"
        android:text="有害垃圾"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arc4" />
    <TextView
        android:id="@+id/zhuangtai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="248dp"
        android:layout_marginTop="568dp"
        android:text="系统状态：正常"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/julidui"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="804dp"
        android:layout_marginTop="572dp"
        android:text="用户距离：正常"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1188dp"
        android:text="操作模式"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/bug"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="28dp"
        android:text="错误报告：0"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="412dp"
        android:layout_marginTop="8dp"
        android:text="其他垃圾"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arc2" />

</androidx.constraintlayout.widget.ConstraintLayout>

